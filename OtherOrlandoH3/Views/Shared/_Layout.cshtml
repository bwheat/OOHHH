<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <link rel="stylesheet" href="~/Content/bootstrap.min.css"/>
    @*@Styles.Render("~/Content/bootstrapcss")*@
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("O2H3", "Index", "Home", new {area = ""}, new {@class = "navbar-brand"})
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li>@Html.ActionLink("Red Dress", "RedDress", "Home")</li>
                <li>@Html.ActionLink("Calendar", "Calendar", "Home")</li>
                <li>@Html.ActionLink("Hare Line", "Hareline", "Home")</li>
                <li>@Html.ActionLink("Mismanagement", "Mismanagement", "Home")</li>
                <li>@Html.ActionLink("Guidelines", "Guidelines", "Home")</li>
                <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
            </ul>
        </div>
    </div>
</div>
<div id="sidebar">
    <div id="NextEvent">
        
    </div>
</div>
@RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
</body>
</html>
